template(name='admin')
	div.container-fluid
		h3 Admin Tools
		div.panel.panel-material-light-blue-400.set-price
			div.panel-heading
				h3.panel-title Set Price
			div.panel-body
				div.form-group
					label.control-label Bedroom
					input(type='number' placeholder='$ 20').form-control#bedroom-price-input
				div.form-group
					label.control-label Bathroom
					input(type='number' placeholder='$ 20').form-control#bathroom-price-input
				div.form-group
					label.control-label Livingroom
					input(type='number' placeholder='$ 20').form-control#livingroom-price-input
				div.form-group
					label.control-label Kitchen
					input(type='number' placeholder='$ 20').form-control#kitchen-price-input
				div.form-group
					label.control-label Basement
					input(type='number' placeholder='$ 20').form-control#basement-price-input
				div.form-group
					label.control-label Garage
					input(type='number' placeholder='$ 20').form-control#garage-price-input
				div.form-group
					label.control-label Yard
					input(type='number' placeholder='$ 20').form-control#yard-price-input
				div.container-fluid.text-center
					a.btn.btn-material-light-blue-400#save-price save