template(name='homepage')
	div.container-fluid.homepage-div.text-center
		div.container
			div.jumbotron
				h3 Superb Cleaning Service
				p
					a(data-toggle='modal' data-target='#sign-in-with').btn.btn-material-light-blue-900.btn-lg sign-up now
		div.container
			p or get a quick estimate		
		div.container
			a(data-toggle='modal' data-target='#residential-modal').cat-btn.btn.btn-primary.btn-raised.btn-material-light-blue-800#residential       Residential
		div.container
			a(href="javascript:void(0)").cat-btn.btn.btn-primary.btn-raised.btn-material-light-blue-700#office office / commercial
		div.container
			a(href="javascript:void(0)").cat-btn.btn.btn-primary.btn-raised.btn-material-light-blue-600#enterprise Enterprise

		// Modals 

		// Sign in With

		div.container
			div.modal.backdrop.up-animate#sign-in-with
				div.modal-dialog
					div.modal-content
						div.modal-header
							button(type='button' data-dismiss='modal' aria-hidden='true').close
								i.fa.fa-times
							h4.modal-title Sign in with
						div.modal-body
							div.container-fluid
								button.accounts-btn.btn.btn-default#google
									i.fa.fa-google.pull-left
									span google
							div.container-fluid
								button.accounts-btn.btn.btn-default#facebook
									i.fa.fa-facebook.pull-left
									span facebook

		// Estimate

		// Residential

		div.container
			div.modal.backdrop.up-animate#residential-modal
				div.modal-dialog
					div.modal-content
						div.modal-header
							button(type='button' data-dismiss='modal' aria-hidden='true').close
								i.fa.fa-times
							h4.modal-title Residential Estimate
							+test
						div.modal-body
							div.list-group
								div.list-group-item
									div.btn-group.btn-group-justified
										a.btn.btn-material-light-blue-800
											i.fa.fa-minus
										a.btn.btn-material-light-blue-50.disabled 1 
										a.btn.btn-material-light-blue-50.disabled.text-left BR
										a.btn.btn-material-light-blue-800.btn-active
											i.fa.fa-plus
								br
								div.list-group-item
									div.btn-group.btn-group-justified
										a.btn.btn-material-light-blue-700
											i.fa.fa-minus
										a.btn.btn-material-light-blue-50.disabled 1 BATH
										a.btn.btn-material-light-blue-700
											i.fa.fa-plus
								br
								div.list-group-item
									div.btn-group.btn-group-justified
										a.btn.btn-material-light-blue-600
											i.fa.fa-minus
										a.btn.btn-material-light-blue-50.disabled 1 LR
										a.btn.btn-material-light-blue-600
											i.fa.fa-plus
								br
								div.list-group-item
									div.container-fluid
										div.checkbox
											label
												input(type='checkbox')
												span kitchen	
										