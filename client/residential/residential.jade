template(name='residential')
	div.container-fluid
		ul.nav.nav-tabs.nav-justified.title-tab
			li
				a
					i.fa.fa-arrow-circle-left.fa-2x.pull-left#back-to-main
			li
				a.disabled.withoutripple
					h4.title-category.text-center.title-category Residential
			li
				a.withoutripple
					i.fa.fa-arrow-circle-right.fa-2x.pull-right
		
	div.container-fluid.text-center
		p Tell us which area needs professional cleaning
		
	br
	each roomCount
		div.container-fluid.alpha-page-div
			ul.nav.nav-tabs.nav-justified.no-background
				li
					p PRICE
				li
					i.fa.fa-dollar#price-estimate &nbsp; #{priceEstimate}
				li
					p DURATION
				li
					p#duration-residential #{durationEstimate} 2 hrs
			br
			ul.nav.nav-tabs.nav-justified#darkest
				li#minus-bedroom
					a
						i.fa.fa-minus
				li
					a.disabled.withoutripple #{bedroom} BEDROOM
				li#plus-bedroom
					a
						i.fa.fa-plus
			br
			ul.nav.nav-tabs.nav-justified#darker
				li#minus-bathroom
					a
						i.fa.fa-minus
				li
					a.disabled.withoutripple #{bathroom} BATHROOM
				li#plus-bathroom
					a
						i.fa.fa-plus
			br
			ul.nav.nav-tabs.nav-justified#dark
				li#minus-livingroom
					a
						i.fa.fa-minus
				li
					a.disabled.withoutripple #{livingroom} LIVINGROOM
				li#plus-livingroom
					a
						i.fa.fa-plus
			br
			ul.nav.nav-tabs#medium
				li.pull-left
					p KITCHEN
				li.pull-right
					div.checkbox
						label
							input(type='checkbox')#kitchen
			br
			ul.nav.nav-tabs#med-light
				li.pull-left
					p BASEMENT
				li.pull-right
					div.checkbox
						label
							input(type='checkbox')#basement-estimate
			br
			ul.nav.nav-tabs#light
				li.pull-left
					p GARAGE
				li.pull-right
					div.checkbox
						label
							input(type='checkbox')#garage-estimate
			br
			ul.nav.nav-tabs#lighter
				li.pull-left
					p YARD / LAWN
				li.pull-right
					div.checkbox
						label
							input(type='checkbox')#yard-estimate
			br




	//div.container-fluid.text-center.input-group.datetimepicker
		span.input-group-addon
		input(type='text').set-due-date.form-control